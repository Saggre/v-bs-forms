{
  "name": "@saggre/v-bs-forms",
  "type": "module",
  "version": "5.2.0-RC7",
  "description": "Typed Bootstrap form builder for Vue3",
  "author": "Sakri Koskimies <sakri.koskimies@hotmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Saggre/v-bs-forms.git"
  },
  "main": "dist/cjs/index.js",
